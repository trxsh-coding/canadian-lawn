{
  "kind": "collectionType",
  "collectionName": "lawns",
  "info": {
    "singularName": "lawn",
    "pluralName": "lawns",
    "displayName": "Lawn"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "speed": {
      "type": "integer",
      "default": 5,
      "min": 1,
      "max": 10
    },
    "resistance": {
      "type": "integer",
      "min": 0,
      "max": 10
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "landing": {
      "type": "component",
      "component": "common.months",
      "repeatable": false
    },
    "price": {
      "type": "component",
      "component": "common.packages",
      "repeatable": true,
      "required": true
    },
    "type": {
      "type": "component",
      "component": "common.lawn-type-percent",
      "repeatable": true,
      "required": true
    },
    "isFeature": {
      "type": "boolean",
      "default": false
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "partners_types": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::partners-type.partners-type",
      "mappedBy": "partners"
    },
    "partner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::partner.partner"
    }
  }
}
